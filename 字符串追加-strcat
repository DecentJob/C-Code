
char* my_strcat(char* dest, const char* src)
{
	char* ret = dest;
	//assert(dest != NULL);
	//assert(src);

	while (*dest != '\0')
	{
		dest++;
	}

	while (*dest++ = *src++)
	{
		;
	}

	return ret;
}

int main()
{
	char arr1[20] = "asdfghj";
	char arr2[] = "hello";
	my_strcat(arr1, arr2);
	printf("%s\n", arr1);
	return 0;
}
